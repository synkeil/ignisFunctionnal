{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
     "globals": {
        "iQ": true,
        "$": true,
        "ActiveXObject": true
    },
    "rules": {
        "no-console": 0,
        "no-alert": 0
      }
}
